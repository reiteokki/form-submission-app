{
    "name": "form-submission-backend",
    "version": "1.0.0",
    "main": "dist/app.js",
    "scripts": {
        "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
        "prisma:migrate": "prisma migrate dev --schema=src/prisma/schema.prisma",
        "prisma:seed": "npm run prisma:generate && ts-node src/prisma/seed.ts",
        "start": "node dist/app.js",
        "dev": "nodemon",
        "build": "tsc"
    },
    "dependencies": {
        "@prisma/client": "^6.16.2",
        "bcrypt": "^6.0.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.2",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "multer": "^2.0.2",
        "mysql2": "^3.15.0",
        "sequelize": "^6.37.7"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/multer": "^2.0.0",
        "@types/node": "^24.5.1",
        "nodemon": "^3.1.10",
        "prisma": "^6.16.2",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.2"
    }
